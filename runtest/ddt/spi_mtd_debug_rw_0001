# @name mtd debug read write test 
# @desc Performance mtd debug erase first, then do read write and then compare
#	The partition number is specified in get_mtd_partition_number.sh
# @requires spi_master

SPI_MTD_RW_0001 source 'common.sh'; part=`get_mtd_partition_number.sh "nand"` || die "error getting mtd part number"; size=`get_mtd_erase_size.sh "$part"` || die "error getting mtd erase size"; mtd_debug_readwrite_test.sh -l "$size" -o "0" -d "spi"





