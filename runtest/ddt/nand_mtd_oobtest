# @name Nand mtd oobtest
# @desc Insmod mtd_oobtest and then rmmod it 
#	The partition number is specified in get_mtd_partition_number.sh
# @requires nand

NAND_L_FUNC_OOBTEST_0001 source 'common.sh'; source 'mtd_common.sh'; part=`get_mtd_partition_number.sh "nand"`; dev_node="$MTD_BLK_DEV$part"; blk_device_do_mount.sh -n "$dev_node" -f "jffs2" -d "nand" -m "/mnt/nand"; do_cmd insmod mtd_oobtest.ko dev="$part"; do_cmd rmmod mtd_oobtest.ko; do_cmd umount "$dev_node" 





