# @name Nand mtdtests and mtd nandecc test
# @desc Nand mtdtests and mtd nandecc test 
# @requires nand

NAND_L_FUNC_MTD_OOBTEST source "common.sh"; install_modules.sh "nand"; dev_node=`get_blk_device_node.sh "nand"` || die "error getting blk device node: $dev_node"; flash_eraseall $dev_node; do_cmd insmod.sh "mtd_oobtest" $dev_node; do_cmd rmmod.sh "mtd_oobtest" 

NAND_L_FUNC_MTD_PAGETEST source "common.sh"; install_modules.sh "nand"; dev_node=`get_blk_device_node.sh "nand"` || die "error getting blk device node: $dev_node"; flash_eraseall $dev_node; do_cmd insmod.sh "mtd_pagetest" $dev_node; do_cmd rmmod.sh "mtd_pagetest" 

NAND_L_FUNC_MTD_READTEST source "common.sh"; install_modules.sh "nand"; dev_node=`get_blk_device_node.sh "nand"` || die "error getting blk device node: $dev_node"; flash_eraseall $dev_node; do_cmd insmod.sh "mtd_readtest" $dev_node; do_cmd rmmod.sh "mtd_readtest" 

NAND_L_FUNC_MTD_SPEEDTEST source "common.sh"; install_modules.sh "nand"; dev_node=`get_blk_device_node.sh "nand"` || die "error getting blk device node: $dev_node"; flash_eraseall $dev_node; do_cmd insmod.sh "mtd_speedtest" $dev_node; do_cmd rmmod.sh "mtd_speedtest" 

NAND_L_FUNC_MTD_STRESSTEST source "common.sh"; install_modules.sh "nand"; dev_node=`get_blk_device_node.sh "nand"` || die "error getting blk device node: $dev_node"; flash_eraseall $dev_node; do_cmd insmod.sh "mtd_stresstest" $dev_node; do_cmd rmmod.sh "mtd_stresstest" 

NAND_L_FUNC_MTD_SUBPAGETEST source "common.sh"; install_modules.sh "nand"; dev_node=`get_blk_device_node.sh "nand"` || die "error getting blk device node: $dev_node"; flash_eraseall $dev_node; do_cmd insmod.sh "mtd_subpagetest" $dev_node; do_cmd rmmod.sh "mtd_subpagetest" 

NAND_L_FUNC_MTD_NANDECCTEST source "common.sh"; install_modules.sh "nand"; dev_node=`get_blk_device_node.sh "nand"` || die "error getting blk device node: $dev_node"; flash_eraseall $dev_node; do_cmd insmod.sh "mtd_nandecctest"; do_cmd rmmod.sh "mtd_nandecctest" 





